type BookVariant {
    id: Int
    book: PublishedBook
    isbn: String
    format: Format
    publishDate: String
    price: Float
}

enum Format {
    PAPERBACK, HARDCOVER, EBOOK
}

type PublishedBook {
    title: String
    genre: Genre
    id: Int
    bookVariants: BookVariant
}

enum Genre {
    SCIENCE_FICTION, HORROR, MYSTERY, ROMANCE, FANTASY
}